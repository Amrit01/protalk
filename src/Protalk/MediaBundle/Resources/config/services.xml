<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="media_content.bliptv">Protalk\MediaBundle\Import\Bliptv</parameter>
        <parameter key="media_content.youtube">Protalk\MediaBundle\Import\YouTube</parameter>
    </parameters>
    
    <services>
        <service id="protalk_media.bliptv" class="%media_content.bliptv%"></service>
        <service id="protalk_media.youtube" class="%media_content.youtube%"></service>
    </services>

</container>
